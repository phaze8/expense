<p id="notice"><%= notice %></p>

<h1>Level1s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @level1s.each do |level1| %>
      <tr>
        <td><%= level1.name %></td>
        <td><%= link_to 'Show', level1 %></td>
        <td><%= link_to 'Edit', edit_level1_path(level1) %></td>
        <td><%= link_to 'Destroy', level1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Level1', new_level1_path %>
